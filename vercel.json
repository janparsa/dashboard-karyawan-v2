{
  "functions": {
    "api/upload-csv.py": {
      "runtime": "@vercel/python"
    },
    "api/index.py": {
      "runtime": "@vercel/python"
    }
  },
  "rewrites": [
    {
      "source": "/upload-csv",
      "destination": "/api/upload-csv"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}